{
  "name": "@cyphr/sdk",
  "version": "1.0.0",
  "description": "SDK for integrating Cyph Wallet with React, Chakra UI, and Wagmi.",
  "keywords": ["web3", "wallet", "react", "chakra-ui", "wagmi", "sdk", "cyph"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CypherVerseLabs/cyph-wallet-sdk.git"
  },
  "files": ["dist"],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.cjs.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.0.0",
    "react": "^18.0.0",
    "wagmi": "^2.16.9"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": [
    "./dist/styles/theme-vars.css"
  ]
}
